{
  "version": 8,
  "name": "MagaDrive Dark Gold",
  "sprite": "https://api.maptiler.com/maps/streets/sprite",
  "glyphs": "https://api.maptiler.com/fonts/{fontstack}/{range}.pbf",
  "sources": {
    "maptiler": {
      "type": "raster",
      "tiles": [
        "https://api.maptiler.com/maps/streets/{z}/{x}/{y}.png?key=${MAPTILER_API_KEY}"
      ],
      "tileSize": 256,
      "attribution": "© MapTiler © OpenStreetMap contributors"
    }
  },
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#0B0B0E"
      }
    },
    {
      "id": "maptiler-raster",
      "type": "raster",
      "source": "maptiler",
      "paint": {
        "raster-opacity": 0.8,
        "raster-saturation": -0.3,
        "raster-contrast": 0.2,
        "raster-brightness-min": 0.1
      }
    }
  ],
  "metadata": {
    "maptiler:copyright": "© MapTiler © OpenStreetMap contributors",
    "maptiler:version": "1.0.0",
    "maptiler:style": "dark_gold"
  }
}
